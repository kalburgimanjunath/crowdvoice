<%= form_for [:admin, @setting] do |f| %>
  <section class="clearfix" style="width:700px; float:left;">
    <div class="clearfix">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="clearfix">
      <%= f.label :value %>
      <%= f.text_field :value %>
    </div>
    <div class="action clearfix">
      <button class="button button-gray" type="submit"><span class="accept"></span>Save</button>
    </div>
  </section>
<% end %>
